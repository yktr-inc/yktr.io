<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}{% endblock %}</title>
          <link rel="stylesheet" type="text/css" href="{{ asset('build/css/app.css') }}">
          <link rel="stylesheet" type="text/css" href="{{ asset('build/css/style.css') }}">
          <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
        {% block stylesheets %}{% endblock %}
    </head>
    <body>
        {% include "Front/layouts/navbar.html.twig" %}
        {% block breadcrum %}{% endblock %}
                      {% for label, messages in app.flashes %}
                {% for message in messages %}
                    <div class="alert alert-dismissible fade show alert-{{ label }}">
                        {{ message }}
                      <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                      </button>
                    </div>
                {% endfor %}
              {% endfor %}
        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
